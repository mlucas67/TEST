SUBROUTINE DIFFINT( MYDIFFS, ARRAYA, ARRAYB )

    CALL DIFFDATA( DATAA, ARRAYA )
    DATAA.LENGTH = DATAA<1>
    CALL DIFFDATA( DATAB, ARRAYB )
    DATAB.LENGTH = DATAB<1>
    DOWNVECTOR = ""
    UPVECTOR = ""
    CALL LCS( DATAA, 1, DATAA.LENGTH, DATAB, 1, DATAB.LENGTH, DOWNVECTOR, UPVECTOR )
    CALL CREATEDIFFS( MYDIFFS, DATAA, DATAB )
    RETURN 

END ;* DIFFINT
