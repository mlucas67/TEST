$debug
regex = object( "!Regex" )

test1.re = "abcdefg"
test1.str = "abcdefg"
code = regex->study( test1.re )
if (regex->re_execute( code, test1.str )) then
    display test1.str : " ~ " : test1.re
end else
    display test1.str : " !~ " : test1.re
end

